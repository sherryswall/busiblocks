<?xml version="1.0"?>
<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
  <system.web>
    <!-- Allow only admin roles -->
    <authorization>
      <allow roles="core:administrator" />
      <allow roles="core:regionadmin" />
      <allow roles="core:siteadmin" />
      <allow roles="docoblock" />
      <allow roles="commsblock" />
      <deny users="*" />
    </authorization>
  </system.web>
</configuration>
