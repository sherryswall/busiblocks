<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="BusiBlocks.ApproverLayer.Approver, BusiBlocks" table="Approver">
    <id name="Id" column="Id" type="String" length="32">
      <generator class="uuid.hex">
        <param name="format">N</param>
      </generator>
    </id>
    
    <property name="ItemId" column="ItemId" type="String" length="32" />
    <property name="UserId" column="UserId" type="String" length="32" />
    <property name="CategoryId" column="CategoryId" type="String" length="32" />
    
    <property name="Deleted" column="Deleted" type="Boolean" />
    
  </class>
</hibernate-mapping>